local mall = game.Workspace:FindFirstChild("Mall")

if mall then
    for _, child in ipairs(mall:GetChildren()) do
        if child:IsA("Model") and child.Name == "Table of Manikins" then
            for _, subChild in ipairs(child:GetChildren()) do
                if subChild:IsA("Model") and subChild.Name == " " then
                    subChild:Destroy()
                end
            end
        end
    end
else
    warn("Mall not found in Workspace.")
end






