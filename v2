local localplayer = game.Players.LocalPlayer
local stroller = localplayer.Backpack.Stroller

local TweenService = game:GetService("TweenService")

local function tweenToVoid(player, duration)
    local character = localplayer.Character
    local rootPart = character:WaitForChild("HumanoidRootPart")
    local goalPosition = Vector3.new(0, -500, 0) -- Void position
    local tweenInfo = TweenInfo.new(duration, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut)

    -- Create and play the Tween
    local tween = TweenService:Create(rootPart, tweenInfo, {Position = goalPosition})
    tween:Play()
end


local tool = localplayer.Backpack.Stroller
local part = tool:WaitForChild("Colr")
local rainbowRunning = false 
local hue = 0
local function HSVToRGB(h, s, v)
    local i = math.floor(h * 6)
    local f = h * 6 - i
    local p = v * (1 - s)
    local q = v * (1 - f * s)
    local t = v * (1 - (1 - f) * s)

    i = i % 6
    if i == 0 then
        return v, t, p
    elseif i == 1 then
        return q, v, p
    elseif i == 2 then
        return p, v, t
    elseif i == 3 then
        return p, q, v
    elseif i == 4 then
        return t, p, v
    elseif i == 5 then
        return v, p, q
    end
end

local function startRainbow()
    rainbowRunning = true
    while rainbowRunning do
        hue = hue + 0.01
        if hue > 1 then hue = 0 end
        local r, g, b = HSVToRGB(hue, 1, 1)
        part.Color = Color3.new(r, g, b)

        task.wait(0.03) 
    end
end


local function stopRainbow()
    rainbowRunning = false
end
